# The look-up table contains 256 entries. Each entry is a floating point sRGB triplet.
# To use it with matplotlib, pass cmap=ListedColormap(turbo_colormap_data) as an arg to imshow() (don't forget "from matplotlib.colors import ListedColormap").
# If you have a typical 8-bit greyscale image, you can use the 8-bit value to index into this LUT directly.
# The floating point color values can be converted to 8-bit sRGB via multiplying by 255 and casting/flooring to an integer. Saturation should not be required for IEEE-754 compliant arithmetic.
# If you have a floating point value in the range [0,1], you can use interpolate() to linearly interpolate between the entries.
# If you have 16-bit or 32-bit integer values, convert them to floating point values on the [0,1] range and then use interpolate(). Doing the interpolation in floating point will reduce banding.
# If some of your values may lie outside the [0,1] range, use interpolate_or_clip() to highlight them.

func interpolate(colormap, x):
  x = max(0.0, min(1.0, x))
  var a = int(x*255.0)
  var b = min(255, a + 1)
  var f = x*255.0 - a
  return Color(colormap[a][0] + (colormap[b][0] - colormap[a][0]) * f,
          colormap[a][1] + (colormap[b][1] - colormap[a][1]) * f,
          colormap[a][2] + (colormap[b][2] - colormap[a][2]) * f,
		  1.0)

func interpolate_or_clip(colormap, x):
  if   x < 0.0: return Color(0.0, 0.0, 0.0, 0.0)
  elif x > 1.0: return Color(1.0, 1.0, 1.0, 1.0)
  else: return interpolate(colormap, x)

var element_count= 10

var bar_elements = []

func set_element_color(mat, color):
	mat.albedo_color = color

func _ready():
	var bar_element = get_node("BarElement")
	var tmp = bar_element.get_node("Active").get_surface_material(0)
	tmp = tmp.duplicate()
	bar_element.get_node("Active").set_surface_material(0,tmp)
	set_element_color(tmp, interpolate_or_clip(blue_colormap_data, 0))
	bar_elements.append(bar_element)
	for i in range(1, element_count):
		var new_element = bar_element.duplicate()
		tmp = new_element.get_node("Active").get_surface_material(0)
		tmp = tmp.duplicate()
		new_element.get_node("Active").set_surface_material(0,tmp)
		set_element_color(tmp, interpolate_or_clip(blue_colormap_data, float(i)/float(element_count-1)))

		bar_elements.append(new_element)
		add_child(bar_elements[-1])
		bar_elements[-1].translation.z = -i*bar_elements[-1].scale.y*3

	set_energy(0)


func set_energy(value):
	var active_elements = int(clamp(value*element_count, 0 , element_count))
	var i = 0
	
	while i < active_elements and i < element_count:
		bar_elements[i].get_node("Active").show()
		i = i +1
	while i < element_count:
		bar_elements[i].get_node("Active").hide()
		i = i + 1
	
